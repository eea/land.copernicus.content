<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
      lang="en-US"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<body>

<metal:description fill-slot="content-description" />

<metal:content-core fill-slot="content-core">
<metal:main_macro define-macro="content-core">
  <dl class="enableFormTabbing">

    <!-- Metadata -->
    <tal:metadata define="
        field python:here.getField('text') if getattr(here, 'getField', None) else None;
        html python:field.getAccessor(here)() if field else ''" tal:condition="html">
      <dt id="fieldsetlegend-metadata" i18n:translate="">Metadata</dt>
      <dd id="fieldset-metadata">
        <div class="metadata-field">
          <tal:service replace="structure html" />
        </div>
      </dd>
    </tal:metadata>

    <!-- Map View -->
    <tal:mapview define="
        field python:here.getField('embed') if getattr(here, 'getField', None) else None;
        html python:field.getAccessor(here)() if field else ''" tal:condition="html">
      <dt id="fieldsetlegend-mapview" i18n:translate="">Map View</dt>
      <dd id="fieldset-mapview">
        <div class="mapview-field">
          <tal:service replace="structure html" />
        </div>
      </dd>
    </tal:mapview>

    <!-- Web services -->
    <tal:webservices define="
        field python:here.getField('webservices') if getattr(here, 'getField', None) else None;
        html python:field.getAccessor(here)() if field else ''" tal:condition="html">
      <dt id="fieldsetlegend-webservices" i18n:translate="">Web Map Services</dt>
      <dd id="fieldset-webservices">
        <div class="webservices-field">
          <tal:service replace="structure html" />
        </div>
      </dd>
    </tal:webservices>

    <!-- Download -->
    <tal:download define="
        field python:here.getField('download') if getattr(here, 'getField', None) else None;
        html python:field.getAccessor(here)() if field else ''" tal:condition="html">
      <dt id="fieldsetlegend-download" i18n:translate="">Download</dt>
      <dd id="fieldset-download">
        <div class="download-field">
          <tal:service replace="structure html" />
        </div>
      </dd>
    </tal:download>

  </dl>

</metal:main_macro>

</metal:content-core>

</body>

</html>
