<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
      lang="en-US"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

<body>
  <metal:content-core fill-slot="content-core">

    <h4 i18n:translate="">Broken landfiles:</h4>
    <tal:errorslist tal:repeat="error python:view.items()">
      <p>
        <a tal:attributes="href python:error[0]"
           i18n:translate="">Landitem</a> -
        <span tal:content="python:error[1]"></span> -
        <span tal:content="python:error[2]"></span>
      </p>
    </tal:errorslist>
  </metal:content-core>
</body>
</html>
